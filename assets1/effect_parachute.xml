<effect name="Parachute">	
	<emitter name="cloud" life="0.9">
		<param name="settings" mode="once" type="area" width="80" height="50"/>
		<param name="image" file="Parachute.png" blend="alpha" width="40" height="40"/>
		<param name="life" min="1.5" max="2.0"/>
		<param name="angle" min="0.0" max="360.0"/>
		<param name="rotangle" type="random" min="0.0" max="0.0"/>
		<param name="speed" min="3.0" max="5.0"/>
		<param name="gravity" min="50.0" max="50.0"/>
		<param name="scale" min="1.0" max="1.0"/>
		<param name="rotspeed" min="0.0" max="0.0"/>
		<param name="r" min="255" max="255"/>
		<param name="g" min="255" max="255"/>
		<param name="b" min="255" max="255"/>
		<param name="a" min="255" max="255"/>	
		<param name="timetrack">
			<track time="0.0" quantity="20.0"/>
			<track time="0.5" quantity="20.0"/>
			<track time="0.9" quantity="0.0"/>
		</param>
		<param name="lifetrack">
			<track life="0.0" speed="0.9" gravity="1.0" scale="0.1" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="1.0"/>
			<track life="0.2" speed="0.9" gravity="1.0" scale="1.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="1.0"/>
			<track life="0.3" speed="0.8" gravity="1.0" scale="1.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="0.6"/>
			<track life="1.0" speed="0.7" gravity="1.0" scale="1.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="0.0"/>
		</param>
	</emitter>
	
</effect>
