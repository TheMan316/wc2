<effect name="gas">
	
	<emitter name="cloud" life="0.2">
		<param name="settings" mode="cont" type="area" width="20" height="20"/>
		<param name="image" file="cloud.png" blend="alpha" width="40" height="40"/>
		<param name="life" min="0.8" max="1.0"/>
		<param name="angle" min="0.0" max="360.0"/>
		<param name="rotangle" type="random" min="0.0" max="360.0"/>
		<param name="speed" min="25.0" max="30.0"/>
		<param name="gravity" min="400.0" max="500.0"/>
		<param name="scale" min="0.8" max="1.0"/>
		<param name="rotspeed" min="0.4" max="1.0"/>
		<param name="r" min="59" max="59"/>
		<param name="g" min="79" max="79"/>
		<param name="b" min="18" max="18"/>
		<param name="a" min="255" max="255"/>	
		<param name="timetrack">
			<track time="0.0" quantity="20.0"/>
			<track time="0.1" quantity="20.0"/>
		</param>
		<param name="lifetrack">
			<track life="0.0" speed="1.0" gravity="0" scale="1.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="0.7"/>
			<track life="0.3" speed="1.0" gravity="0" scale="2.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="0.6"/>
			<track life="1.0" speed="1.0" gravity="0" scale="2.0" rotspeed="1.0" r="1.0" g="1.0" b="1.0" a="0.0"/>
		</param>
	</emitter>
	
</effect>
